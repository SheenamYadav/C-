#include<iostream>
#include<stack>
using namespace std;
int main()
{
    int n;
    int dgt;
    stack <int> stk;
    cout<< "Enter the digits the number has: ";
    cin>>n;
    cout<<"Enter the digits one by one:";
    for (int i =0;i<n;i++)
    {
        cin>>dgt;
        stk.push(dgt);
    }
    cout<<"Reversed Number is : ";
    while(!stk.empty())
    {
        cout<<stk.top();
        stk.pop();
    }    
}


